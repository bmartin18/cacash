{% extends "/base.html.twig" %}

{% block title %}
    Erreur {{ status_code }}
{% endblock title %}

{% block body %}
    <div class="row">
        <div class="col s12 m6 offset-m3 l6 offset-l3 xl4 offset-xl4">
            <div class="card">
                <div class="card-image">
                    <img src="{{ asset('images/card/cacash.jpg') }}">
                </div>
                <div class="card-content">
                    <span class="card-title">Error {{ status_code }}</span>
                    <p>{{ status_text }}</p>
                </div>
            </div>
        </div>
    </div>
{% endblock body %}
